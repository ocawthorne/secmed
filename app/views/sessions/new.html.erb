<h4>Sign in (patients and practitioners)</h4>
<br>

<%= form_tag('/login', method: :post) do %>

  <div class="form-group">
    <%= label_tag(:email) %>
    <%= text_field_tag(:email, nil, class: "form-control") %>
  </div>

  <div class="form-group">
    <%= label_tag(:password) %>
    <%= password_field_tag(:password, nil, class: "form-control") %>
  </div>

  <div class="actions">
    <%= submit_tag("Sign In") %>
  </div>

<% end %> 
